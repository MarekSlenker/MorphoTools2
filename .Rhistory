# internal
# returns new pcadata object
newCdadata <- function() {
newCdadata <- list(
axesVariance = numeric(),
cumulativeAxesVariance = numeric(),
groupMeans = data.frame(),
coeffs.raw = matrix(),
coeffs.std = matrix(),
totalCanonicalStructure = matrix(),
objects = list(
ID = factor(),
Population = factor(),
Taxon = factor(),
scores = data.frame()),
rank = numeric(),
eigenValues = numeric(),
canrsq = numeric()
)
attr(newCdadata, "class") <- "cdadata"
return(newCdadata)
}
